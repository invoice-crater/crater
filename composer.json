{
  "name": "crater-invoice/crater",
  "description": "Free & Open Source Invoice App for Individuals & Small Businesses. https://craterapp.com",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.1",
    "aws/aws-sdk-php": "^3.296.6",
    "barryvdh/laravel-dompdf": "^2.0.1",
    "crater-invoice/modules": "^1.0",
    "doctrine/dbal": "^3.7.2",
    "dragonmantank/cron-expression": "^3.3.3",
    "guzzlehttp/guzzle": "^7.8.1",
    "intervention/image": "^2.7.2",
    "jasonmccreary/laravel-test-assertions": "^2.3",
    "laravel/framework": "^10.41",
    "laravel/helpers": "^1.7",
    "laravel/sanctum": "^3.3.3",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.4",
    "lavary/laravel-menu": "^1.8.4",
    "league/flysystem-aws-s3-v3": "^3.22",
    "predis/predis": "^2.2.2",
    "silber/bouncer": "^1.0.1",
    "spatie/flysystem-dropbox": "^3.0.1",
    "spatie/laravel-backup": "^8.5",
    "spatie/laravel-medialibrary": "^10.15",
    "vinkla/hashids": "^11.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "beyondcode/laravel-dump-server": "^1.9",
    "friendsofphp/php-cs-fixer": "^3.48",
    "fakerphp/faker": "^1.23.1",
    "mockery/mockery": "^1.6.7",
    "nunomaduro/collision": "^6.4",
    "pestphp/pest": "^1.23.1",
    "pestphp/pest-plugin-faker": "^1.0",
    "pestphp/pest-plugin-laravel": "^1.4",
    "pestphp/pest-plugin-parallel": "^1.2.1",
    "phpunit/phpunit": "^9.6.16",
    "spatie/laravel-ignition": "^2.4.1"
  },
  "autoload": {
    "psr-4": {
      "Crater\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/Space/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
